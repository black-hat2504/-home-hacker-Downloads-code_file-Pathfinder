import java.net.*;
class UDPServer {
  public static void main(String[] a)throws Exception{
    DatagramSocket s=new DatagramSocket(9876);
    byte[] r=new byte[100],m;
    DatagramPacket p=new DatagramPacket(r,r.length);
    s.receive(p);
    System.out.println("Client: "+new String(p.getData()).trim());
    m="Hello Client".getBytes();
    s.send(new DatagramPacket(m,m.length,p.getAddress(),p.getPort()));
    s.close();
  }
}